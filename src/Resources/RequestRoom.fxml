<GridPane xmlns="http://javafx.com/javafx/8.0.112"  xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.RequestRoomController">
<children>
   <VBox alignment="CENTER_LEFT" prefHeight="500" prefWidth="1270"  spacing="20.0" translateY="200.0">
      <children>
         <GridPane translateX="500">
         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Requst Room">
            <font>
               <Font size="36.0" />
            </font>
         </Text>
      </GridPane>

         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Purpose">
            <font>
               <Font size="20.0" />
            </font>
         </Text>
         <TextField alignment="TOP_LEFT" layoutX="144.0" layoutY="116.0" prefHeight="144.0" prefWidth="442.0" />

         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Room Required">
            <font>
               <Font size="20.0" />
            </font>
         </Text>
         <TextField/>
         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Student Capacity">
            <font>
               <Font size="20.0" />
            </font>
         </Text>
         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="(optional)" />
         <TextField/>
         <Button mnemonicParsing="false" onAction="#submitrequest" prefHeight="52.0" prefWidth="138.0" text="Submit" translateX="500" />

      </children>
   </VBox>
</children>
<columnConstraints>
   <ColumnConstraints />
</columnConstraints>
<rowConstraints>
   <RowConstraints />
